@model ML.Tranferencia
@{
    ViewData["Title"] = "TransactionGetByAccount";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>GetAll</h1>
<table class="table">
    <thead>
        <tr>
            <th scope="col">FromAccount</th>
            <th scope="col">ToAccount</th>
            <th scope="col">Amount</th>
            <th scope="col">FechaEnvio</th>
        </tr>
    </thead>
    <tbody>
        @foreach (ML.Tranferencia cuenta in Model.TranferenciaList)
        {
            <tr>
                <td>@cuenta.FromAccount</td>
                <td>@cuenta.ToAccount</td>
                <td>@cuenta.Amount</td>
                <td>@cuenta.FechaEnvio</td>
            </tr>
        }

    </tbody>
</table>

